{
  "name": "diablo3-leaderboard-bot",
  "version": "1.0.0",
  "description": "Discord bot which will allow a server to view the current state of any single player leaderboard.",
  "main": "bot.js",
  "scripts": {
    "start": "node dist/bot.js",
    "dev": "nodemon dist/bot.js",
    "tsc": "tsc",
    "lint:fix": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "docker": "docker-compose up --build"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "axios": "^0.19.0",
    "discord.js": "^11.5.1",
    "dotenv": "^8.1.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "tslint": "^5.20.0"
  },
  "keywords": [
    "diablo3",
    "leaderboards",
    "leaderbord",
    "discord",
    "discord-bot"
  ],
